#include<bits/stdc++.h>
using namespace std;

int main() {
	int t; cin >> t;
	while(t--) {
		int n; cin >> n;
		int a[n], b[n];
		for(int &i : a) cin >> i;
		stack<int> st;
		for(int i = 0; i < n; ++i) {
			while(st.size() && a[st.top()] <= a[i]) st.pop();
			if(st.size()) b[i] = i - st.top();
			else b[i] = i + 1;
			st.push(i);
		}
		for(int i = 0; i < n; ++i) cout << b[i] << " ";
		cout << endl;
	}
}
