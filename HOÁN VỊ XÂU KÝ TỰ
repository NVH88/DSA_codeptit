#include<bits/stdc++.h>
using namespace std;

char a[15];
int x[15], ok, n, used[15];

void Try(int i) {
	for(int j = 0; j < n; ++j) {
		if(!used[j]) {
			x[i] = j;
			used[j] = 1;
			if(i == n - 1) {
				for(int k = 0; k < n; k++) cout << a[x[k]];
				cout << " ";
			}
			else Try(i + 1);
			used[j] = 0;
		}
	}
}

int main() {
	int t; cin >> t;
	while(t--) {
		string s; cin >> s;
		for(int i = 0; i < s.length(); ++i) a[i] = s[i];
		n = s.length();
		sort(a, a + n);
		Try(0);
		cout << endl;
	}
}
