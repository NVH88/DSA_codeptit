#include<bits/stdc++.h>
using namespace std;
#define ll long long

int main() {
	int t;
	cin >> t;
	while(t--) {
		int n; cin >> n;
		int a[n];
		map<int, int> mp;
		int max = 0, ans;
		for(int i = 0; i < n; i++) {
			cin >> a[i];
			mp[a[i]]++;
			if(mp[a[i]] > max) {
				ans = a[i];
				max = mp[a[i]];
			}
		}
		if(max > n / 2) cout << ans << endl;
		else cout << "NO" << endl;
	}
}
