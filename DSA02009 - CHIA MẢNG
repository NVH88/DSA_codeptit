#include<bits/stdc++.h>
using namespace std;

int main() {
	int t; cin >> t;
	while(t--) {
		int n, k; cin >> n >> k;
		int a[n + 1], sum = 0;
		for(int i = 1; i <= n; ++i) {
			cin >> a[i];
			sum += a[i];
		}
		if(sum % k) {
			cout << 0 << endl;
			continue;
		}
		int dp[sum + 1] = {0}, x = sum / k;
		dp[0] = 1;
		for(int i = 1; i <= n; ++i) {
			for(int j = sum; j >= a[i]; --j) {
				if(dp[j - a[i]]) dp[j] = 1;
			}
		}
		bool check = true;
		for(int i = x; i <= sum; i += x) {
			if(!dp[i]) check = false;
		}
		cout << check << endl;
	}
}
