#include<bits/stdc++.h>
using namespace std;
#define ll long long

int main() {
	int t;
	cin >> t;
	while(t--) {
		int n;
		cin >> n;
		ll a[n];
		for(ll &i : a) cin >> i;
		ll kl = 1e18, ans;
		for(int i = 0; i < n - 1; i++) {
			for(int j = i + 1; j < n; j++) {
				if(abs(a[i] + a[j]) < kl) {
					kl = abs(a[i] + a[j]);
					ans = a[i] + a[j];
				}
			}
		}
		cout << ans << endl;
	}
}
