#include <bits/stdc++.h>
using namespace std;
#define ll long long
const int mod = 1e9 + 7;

int main() {
	int t; cin >> t;
	while(t--) {
	    ll n, x;
	    cin >> n >> x;
	    ll a[n];
	    for(int i = 0; i < n; i++) {
	    	cin >> a[i];
		}
		sort(a, a + n);
		if(a[0] > x) cout << -1 << endl;
		else {
			auto it = lower_bound(a, a + n, x);
			if(*it == x) cout << it - a + 1 << endl;
			else cout << it - a << endl;
		}
	}
}
