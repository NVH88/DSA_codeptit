#include<bits/stdc++.h>
using namespace std;

int main() {
	int t; cin >> t;
	while(t--) {
		int n; cin >> n;
		int a[n], s1[n], s2[n];
		for(int i = 0; i < n; i++) {
			cin >> a[i];
			s1[i] = a[i]; 
			s2[i] = a[i];
		}
		for(int i = 1; i <= n - 1; i++) {
			for(int j = 0; j < i; j++) {
				if(a[j] < a[i]) s1[i] = max(s1[i], s1[j] + a[i]);
			}
		}
		for(int i = n - 2; i >= 0; i--) {
			for(int j = n - 1; j > i; j--) {
				if(a[j] < a[i]) s2[i] = max(s2[i], s2[j] + a[i]);
			}
		}
		int ans = 0;
		for(int i = 0; i < n; i++) {
			ans = max(ans, s1[i] + s2[i] - a[i]);
		}
		cout << ans << endl;
	}
}
