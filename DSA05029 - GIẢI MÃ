#include<bits/stdc++.h>
using namespace std;

bool check(string s) {
	if(s.length() == 1) return s != "0";
	else {
		if(s[0] == '1') return true;
		else if(s[0] == '2' && s[1] <= '6') return true;
	}
	return false;
}

int calc(string s) {
	int len = s.length() + 1;
	int dp[len]; memset(dp, 0, sizeof(dp)); dp[0] = 1;
	s = " " + s;
	for(int i = 1; i <= len - 1; ++i) {
		if(check(string(1, s[i]))) dp[i] = dp[i - 1];
		if(check(string(1, s[i - 1]) + string(1, s[i]))) dp[i] += dp[i - 2];
		if(!dp[i]) return 0;
	}
	return dp[len - 1];
}

int main() {
	int t; cin >> t;
	while(t--) {
		string s; cin >> s;
		cout << calc(s) << endl;
	}
}
