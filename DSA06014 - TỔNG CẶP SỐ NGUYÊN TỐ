#include<bits/stdc++.h>
using namespace std;

int X[1000001];

void sang() {
	X[0] = X[1] = 0;
	for(int i = 2; i < 1000001; i++) X[i] = 1;
	for(int i = 2; i < sqrt(1000001); i++) {
		if(X[i]) {
			for(int j = i * i; j < 1000001; j+=i) {
				X[j] = 0;
			}
		}
	}
}

int main() {
	int t;
	cin >> t;
	sang();
	while(t--) {
		int n; cin >> n;
		bool check = false;
		for(int i = 2; i <= n / 2; i++) {
			if(X[i] && X[n - i]) {
				check = true;
				cout << i << " " << n - i << endl;
				break;
			}
		}
		if(!check) cout << -1 << endl;
	}
}
