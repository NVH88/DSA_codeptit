#include<bits/stdc++.h>
using namespace std;

int n, k, a[100];

void Try(int i) {
	for(int j = a[i - 1] + 1; j <= n - k + i; j++) {
		a[i] = j;
		if(i == k) {
			for(int idx = 1; idx <= k; idx++) cout << a[idx];
			cout << " ";
		}
		else Try(i + 1);
	}
}

int main() {
	int t; cin >> t;
	while(t--) {
		cin >> n >> k;
		Try(1);
		cout << endl;
	}
}
