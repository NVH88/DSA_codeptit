#include<bits/stdc++.h>
using namespace std;

int main() {
	int t; cin >> t;
	while(t--) {
		int n, k, s = 0; cin >> n >> k;
		int a[n + 1];
		for(int i = 1; i <= n; ++i) {
			cin >> a[i]; a[i] %= k;
			s += a[i];
		}
		int dp[s + 1] = {0}; dp[0] = 1;
		for(int i = 1; i <= n; ++i) {
			for(int j = s; j >= a[i]; --j) {
				if(dp[j - a[i]]) dp[j] = max(dp[j], dp[j - a[i]] + 1);
			}
		}
		int ans = 0;
		for(int i = 0; i <= s; i += k) {
			ans = max(ans, dp[i] - 1);
		}
		cout << ans << endl;
	}
}
