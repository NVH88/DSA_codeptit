#include<bits/stdc++.h>
using namespace std;

int n, k, a[20], x[20];

void Try(int i) {
	for(int j = a[i - 1] + 1; j <= n - k + i; ++j) {
		a[i] = j;
		if(i == k) {
			for(int p = 1; p <= k; ++p) cout << x[a[p]] << " ";
			cout << endl;
		}
		else Try(i + 1);
	}
}

int main() {
	int t; cin >> t;
	while(t--) {
		cin >> n >> k;
		for(int i = 1; i <= n; ++i) cin >> x[i];
		sort(x + 1, x + 1 + n);
		Try(1);
	}
}
