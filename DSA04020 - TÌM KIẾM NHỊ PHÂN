#include <bits/stdc++.h>
using namespace std;
#define ll long long
const int mod = 1e9 + 7;

int main() {
	int t; cin >> t;
	while(t--) {
	    int n, k;
	    cin >> n >> k;
	    int a[n];
	    for(int i = 0; i < n ;i++) cin >> a[i];
	    sort(a, a + n);
	    auto it = lower_bound(a, a + n, k);
	    if(*it == k) cout << it - a + 1 << endl;
	    else cout << "NO" << endl;
	}
}
