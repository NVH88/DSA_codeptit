#include<bits/stdc++.h>
using namespace std;

int main() {
	int n;
	cin >> n;
	int a[n];
	for(int i = 0; i < n; i++) cin >> a[i];
	int cnt = 1;
	for(int i = 0; i < n - 1; i++) {
		int idx = i;
		for(int j = i + 1; j < n; j++) {
			if(a[j] < a[idx]) {
				idx = j;
			}
		}
//		if(i != idx) {
			swap(a[i], a[idx]);
			cout << "Buoc " << i + 1 << ": ";
			for(int k = 0; k < n; k++) cout << a[k] << " ";
			cout << endl;
//		}
	}
}
