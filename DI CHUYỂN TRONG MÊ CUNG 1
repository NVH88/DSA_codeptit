#include<bits/stdc++.h>
using namespace std;

int n, a[20][20], ok;

void Try(string s, int i, int j) {
	if(i == n && j == n) {
		ok = 1;
		cout << s << " ";
	}
	if(i + 1 <= n && a[i + 1][j] == 1) Try(s + "D", i + 1, j);
	if(j + 1 <= n && a[i][j + 1] == 1) Try(s + "R", i, j + 1);
}

int main() {
	int t; cin >> t;
	while(t--) {
		ok = 0;
		cin >> n;
		for(int i = 1; i <= n; ++i) {
			for(int j = 1; j <= n; ++j) {
				cin >> a[i][j];
			}
		}
		if(!a[1][1]) cout << -1;
		else {
			Try("", 1, 1);
			if(!ok) cout << -1;
		}
		cout << endl;
	}
}
