#include<bits/stdc++.h>
using namespace std;

bool cmp(pair<int, int> a1, pair<int, int> a2) {
	return a1.second < a2.first;
}

bool cmp2(pair<int, int> a1, pair<int, int> a2) {
	return a1.first < a2.first;
}

int main() {
	int t; cin >> t;
	while(t--) {
		int n; cin >> n;
		pair<int, int> a[n];
		int dp[n];
		for(int i = 0; i < n; i++) {
			cin >> a[i].first >> a[i].second;
			dp[i] = 1;
		}
		sort(a, a + n, cmp2);
		for(int i = 1; i < n; i++) {
			for(int j = 0; j < i; j++) {
				if(cmp(a[j], a[i])) dp[i] = max(dp[i], dp[j] + 1);
			}
		}
		cout << *max_element(dp, dp + n) << endl;
	}
}
