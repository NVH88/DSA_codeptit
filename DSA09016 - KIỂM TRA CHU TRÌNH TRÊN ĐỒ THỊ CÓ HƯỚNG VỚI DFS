#include<bits/stdc++.h>
using namespace std;

vector<int> ke[1005];
int n, m, color[1005];

bool dfs(int u) {
	color[u] = 1;
	for(int i : ke[u]) {
		if(!color[i]) { if(dfs(i)) return true; }
		else if(color[i] == 1) return true;
	}
	color[u] = 2;
	return false;
}

int main() {
	int t; cin >> t;
	while(t--) {
		cin >> n >> m;
		for(int i = 1; i <= n; ++i) ke[i].clear();
		memset(color, 0, sizeof(color));
		for(int i = 0; i < m; ++i) {
			int a, b; cin >> a >> b;
			ke[a].push_back(b);
		}
		int ok = 0;
		for(int i = 1; i <= n; ++i) {
			if(!color[i]) {
				if(dfs(i)) { cout << "YES" << endl; ok = 1; break;}
			}
		}
		if(!ok) cout << "NO" << endl;
	}
}
