#include<bits/stdc++.h>
using namespace std;

int x[100005];

void solve() {
	x[1] = 0;
	for(int i = 2; i <= 100000; ++i) {
		x[i] = x[i - 1] + 1;
		if(i % 3 == 0) x[i] = min(x[i], x[i / 3] + 1);
		if(i % 2 == 0) x[i] = min(x[i], x[i / 2] + 1);
	}
}

int main() {
	solve();
	int t; cin >> t;
	while(t--) {
		int n; cin >> n;
		cout << x[n] << endl;
	}
}
