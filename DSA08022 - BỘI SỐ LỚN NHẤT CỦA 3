#include<bits/stdc++.h>
using namespace std;

int a[100005], n, sum;

bool cmp(int a, int b) {
	return a > b;
}

void solve() {
	if(sum % 3 == 0) {
		for(int i = 0; i < n; i++) {
			cout << a[i];
		}
		return;
	}
	if(sum % 3 == 1) {
		vector<int> v;
		for(int i = n - 1; i >= 0; i--) {
			if(a[i] % 3 == 1) {
				for(int j = 0; j < n; j++) {
					if(j != i) {
						cout << a[j];
					}
				}
				return;
			}
			if(a[i] % 3 == 2 && v.size() < 3) {
				v.push_back(i);
			}
		}
		if(v.size() < 2 || n <= 2) cout << -1;
		else {
			for(int j = 0; j < n; j++) {
				if(j != v[0] && j != v[1]) {
					cout << a[j];
				}
			}
		}
	}
	if(sum % 3 == 2) {
		vector<int> v;
		for(int i = n - 1; i >= 0; i--) {
			if(a[i] % 3 == 2) {
				for(int j = 0; j < n; j++) {
					if(j != i) {
						cout << a[j];
					}
				}
				return;
			}
			if(a[i] % 3 == 1 && v.size() < 3) {
				v.push_back(i);
			}
		}
		if(v.size() < 2 || n <= 2) cout << -1;
		else {
			for(int j = 0; j < n; j++) {
				if(j != v[0] && j != v[1]) {
					cout << a[j];
				}
			}
		}
	}
}

int main() {
	int t; cin >> t;
	while(t--) {
		cin >> n;
		sum = 0;
		for(int i = 0; i < n; i++) {
			cin >> a[i];
			sum += a[i];
		}
		sort(a, a + n, cmp);
		solve();
		cout << endl;
	}
}
