#include<bits/stdc++.h>
using namespace std;
#define ll long long

ll chuyen(string s) {
	reverse(s.begin(), s.end());
	ll res = 0;
	for(int i = 0; i < s.length(); i++) {
		res += (s[i] - '0') * pow(2, i);
	}
	return res;
}

int main() {
	int t; cin >> t;
	while(t--) {
		string a, b; cin >> a >> b;
		cout << chuyen(a) * chuyen(b) << endl;
	}
}
