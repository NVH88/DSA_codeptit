#include<bits/stdc++.h>
using namespace std;
#define ll unsigned long long

ll a[100005], n, k;

bool check() {
	if(k == 0) {
		for(int i = 0; i < n; i++) if(a[i] == 0) return true;
		return false;
	}
	ll sum = 0, l = 0, r = 0;
	while(r < n) {
		if(sum == k) return true;
		else if(sum > k) {
			sum -= a[l];
			++l;
		}
		else {
			sum += a[r];
			++r;
		}
	}
	while(l < n) {
		if(sum == k) return true;
		sum -= a[l]; ++l;
	}
	return false;
}

int main() {
	int t; cin >> t;
	while(t--) {
		cin >> n >> k;
		for(int i = 0; i < n; i++) cin >> a[i];
		if(check()) cout << "YES" << endl;
		else cout << "NO" << endl;
	}
}
