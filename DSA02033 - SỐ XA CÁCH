#include<bits/stdc++.h>
using namespace std;

int n, a[15], used[15];

void Try(int i) {
	for(int j = 1; j <= n; ++j) {
		if(!used[j] && (abs(j - a[i - 1]) != 1 || i == 1)) {
			used[j] = 1;
			a[i] = j;
			if(i == n) {
				for(int k = 1; k <= n; ++k) cout << a[k];
				cout << endl;
			}
			else Try(i + 1);
			used[j] = 0;
		}
	}
}

int main() {
	int t; cin >> t;
	while(t--) {
		cin >> n;
		Try(1);
	}
}
