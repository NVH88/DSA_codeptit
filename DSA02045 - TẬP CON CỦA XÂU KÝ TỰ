#include<bits/stdc++.h>
using namespace std;

int n;
string s;
vector<string> v;

void Try(int i, string x) {
	for(int j = i; j < n; ++j) {
		v.push_back(x + string(1, s[j]));
		Try(j + 1, x + string(1, s[j]));
	}
}

int main() {
	int t; cin >> t;
	while(t--) {
		cin >> n >> s;
		Try(0, "");
		sort(v.begin(), v.end());
		for(string i : v) cout << i << " ";
		cout << endl;
		v.clear();
	}
}
