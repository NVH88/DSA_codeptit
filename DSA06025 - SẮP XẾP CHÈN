#include<bits/stdc++.h>
using namespace std;

int main() {
	int n;
	cin >> n;
	int a[n];
	for(int i = 0; i < n; i++) cin >> a[i];
	int cnt = 1;
	cout << "Buoc 0: " << a[0] << endl;
	for(int i = 1; i < n; i++) {
		int idx = i, tmp = a[i];
		while(idx > 0 && a[idx - 1] > tmp) {
			a[idx] = a[idx - 1];
			idx--;
		}
		a[idx] = tmp;
		cout << "Buoc " << i << ": ";
		for(int j = 0; j <= i; j++) {
			cout << a[j] << " ";
		}
		cout << endl;
	}
}
