#include<bits/stdc++.h>
using namespace std;
#define ll long long
const int mod = 1e9 + 7;
ll F[93];

void Fibo() {
	F[1] = 1;
	F[2] = 1;
	for(int i = 3; i <= 92; i++) {
		F[i] = F[i - 1] + F[i - 2];
	}
}

char Find(ll n, ll k) {
	if(n == 1) return 'A';
	if(n == 2) return 'B';
	if(k <= F[n - 2]) return Find(n - 2, k);
	return Find(n - 1, k - F[n - 2]);
}

int main() {
	Fibo();
	int t; cin >> t;
	while(t--) {
		ll n, k; cin >> n >> k;
		cout << Find(n, k) << endl;
	}
}
