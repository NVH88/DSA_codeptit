#include<bits/stdc++.h>
using namespace std;

void solve(int n) {
	bool check = false;
	int k = n / 7;
	for(int i = k; i >= 0; i--) {
		if((n - i * 7) % 4 == 0) {
			check = true;
			k = i;
			break;
		}
	}
	if(!check) cout << -1 << endl;
	else {
		int x = (n - k * 7) / 4;
		for(int i = 1; i <= x; i++) {
			cout << 4;
		}
		for(int i = 1; i <= k; i++) {
			cout << 7;
		}
		cout << endl;
	}
}

int main() {
	int t; cin >> t;
	while(t--) {
		int n; cin >> n;
		solve(n);
	}
}
