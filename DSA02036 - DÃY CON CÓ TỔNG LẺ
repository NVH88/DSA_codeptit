#include<bits/stdc++.h>
using namespace std;

int n, a[20];
vector<vector<int>> v;

bool cmp(int a, int b) {
	return a > b;
}

void Try(int i, int sum, vector<int> vt) {
	for(int j = i; j <= n; ++j) {
		vt.push_back(a[j]);
		if((sum + a[j]) % 2) {
			v.push_back(vt);
		}
		Try(j + 1, sum + a[j], vt);
		vt.pop_back();
	}
}

int main() {
	int t; cin >> t;
	while(t--) {
		cin >> n;
		for(int i = 1; i <= n; ++i) cin >> a[i];
		sort(a + 1, a + n + 1, cmp);
		vector<int> vt;
		Try(1, 0, vt);
		sort(v.begin(), v.end());
		for(vector<int> i : v) {
			for(int j : i) cout << j << " ";
			cout << endl;
		}
		v.clear();
	}
}
