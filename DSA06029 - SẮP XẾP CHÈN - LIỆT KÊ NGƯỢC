#include<bits/stdc++.h>
using namespace std;

int main() {
	int n;
	cin >> n;
	int a[n];
	for(int i = 0; i < n; i++) cin >> a[i];
	vector<string> v;
	string s1 = "Buoc 0: ";
	s1 += to_string(a[0]);
	v.push_back(s1);
	for(int i = 1; i < n; i++) {
		int idx = i, tmp = a[i];
		while(idx > 0 && a[idx - 1] > tmp) {
			a[idx] = a[idx - 1];
			idx--;
		}
		a[idx] = tmp;
		string s = "Buoc ";
		s += to_string(i) + ": ";
		for(int j = 0; j <= i; j++) {
			s += to_string(a[j]) + " ";
		}
		v.push_back(s);
	}
	reverse(v.begin(), v.end());
	for(string i : v) cout << i << endl;
}

