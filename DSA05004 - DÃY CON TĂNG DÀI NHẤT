#include<bits/stdc++.h>
using namespace std;

int main() {
	int n; cin >> n;
	int a[n], dp[n];
	for(int i = 0; i < n; ++i) {
		cin >> a[i]; dp[i] = 1;
	}
	for(int i = 0; i < n; ++i) {
		for(int j = 0; j < i; ++j) {
			if(a[i] > a[j]) dp[i] = max(dp[i], dp[j] + 1);
		}
	}
	cout << *max_element(dp, dp + n);
}

#include<bits/stdc++.h>
using namespace std;

int main() {
	int n; cin >> n;
	int a[n];
	for(int &i : a) cin >> i;
	vector<int> v;
	for(int i = 0; i < n; i++) {
		auto it = lower_bound(v.begin(), v.end(), a[i]);
		if(it == v.end()) v.push_back(a[i]);
		else *it = a[i];
	}
	cout << v.size();
}
