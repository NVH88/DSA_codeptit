#include<bits/stdc++.h>
using namespace std;
#define ll long long
int a[100001], n, k;

int bs1(int a[], int idx, int k) {
	int l = idx + 1, r = n - 1, ans = -1;
	while(l <= r) {
		int m = (l + r) / 2;
		if(a[m] == k) {
			ans = m;
			r = m - 1;
		}
		else if(a[m] < k) {
			l = m + 1;
		}
		else {
			r = m - 1;
		}
	}
	return ans;
}

int bs2(int a[], int idx, int k) {
	int l = idx + 1, r = n - 1, ans = -1;
	while(l <= r) {
		int m = (l + r) / 2;
		if(a[m] == k) {
			ans = m;
			l = m + 1;
		}
		else if(a[m] < k) {
			l = m + 1;
		}
		else {
			r = m - 1;
		}
	}
	return ans;
}

int main() {
	int t;
	cin >> t;
	while(t--) {
		cin >> n >> k;
		for(int i = 0; i < n; i++) {
			cin >> a[i];
		}
		sort(a, a + n);
		ll cnt = 0;
		for(int i = 0; i < n - 1; i++) {
			if(bs1(a, i, k - a[i]) != -1) {
				cnt += (bs2(a, i, k - a[i]) - bs1(a, i, k - a[i]) + 1);
			}
		}
		cout << cnt << endl;
	}
}
