#include <bits/stdc++.h>
using namespace std;
#define ll long long
const int mod = 1e9 + 7;

int main() {
	int t; cin >> t;
	while(t--) {
	    int n;
	    cin >> n;
	    int a[n];
	    for(int i = 0; i < n; i++) {
	    	cin >> a[i];
		}
		int res = -1e9;
		for(int i = 1; i < n; i++) {
			if(a[i] = max(a[i], a[i] + a[i - 1]));
			res = max(res, a[i]);
		}
		cout << res << endl;
	}
}
