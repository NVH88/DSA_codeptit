#include<bits/stdc++.h>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
	cin.tie(nullptr);
	cout.tie(nullptr);
	int t; cin >> t;
	while(t--) {
		int n; cin >> n;
		int a[n], l[n], r[n];
		for(int i = 0; i < n; i++) cin >> a[i];
		l[0] = a[0];
		r[n - 1] = a[n - 1];
		for(int i = 1; i < n; i++) {
			l[i] = max(a[i], l[i - 1]);
		}
		for(int i = n - 2; i >= 0; i--) {
			r[i] = min(a[i], r[i + 1]);
		}
		vector<int> v;
		for(int i = 0; i < n - 1; i++) {
			if(l[i] <= r[i + 1]) v.push_back(i + 1);
		}
		cout << v.size() << endl;
		for(int i : v) cout << i << " ";
		cout << endl;
	}
}
