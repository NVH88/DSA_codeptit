#include<bits/stdc++.h>
using namespace std;
#define ll long long

int main() {
	int t;
	cin >> t;
	while(t--) {
		int n, k;
		cin >> n >> k;
		int a[n];
		bool check = false;
		for(int i = 0; i < n; i++) {
			cin >> a[i];
			if(a[i] == k) check = true;
		}
		if(check) cout << 1 << endl;
		else cout << -1 << endl;
	}
}
