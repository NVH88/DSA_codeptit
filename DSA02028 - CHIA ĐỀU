#include<bits/stdc++.h>
using namespace std;

int n, k, a[20], sum, cnt;

void Try(int i, int s, int dem) {
	if(i == n + 1 && dem == k && s == 0) {
		++cnt;
	}
	if(i == n + 1) return;
	for(int j = i; j <= n; ++j) {
		s += a[j];
		if(s == sum) {
			Try(j + 1, 0, dem + 1);
		}
	}
}

int main() {
	cin >> n >> k;
	for(int i = 1; i <= n; ++i) {
		cin >> a[i];
		sum += a[i];
	}
	if(sum % k) {
		cout << 0;
		return 0;
	}
	sum /= k;
	Try(1, 0, 0);
	cout << cnt;
}
