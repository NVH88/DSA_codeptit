#include<bits/stdc++.h>
using namespace std;
#define ll long long

int main() {
	int t; cin >> t;
	while(t--) {
		string s; cin >> s;
		ll ans = 0;
		for(int n = 1; n <= s.length(); n++) {
			for(int i = 0; i <= s.length() - n; i++) {
				ans += stoll(s.substr(i, n));
			}
		}
		cout << ans << endl;
	}
}
