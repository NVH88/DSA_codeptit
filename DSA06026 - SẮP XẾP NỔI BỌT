#include<bits/stdc++.h>
using namespace std;

int main() {
	int n;
	cin >> n;
	int a[n];
	for(int i = 0; i < n; i++) cin >> a[i];
	for(int i = 0; i < n; i++) {
		bool check = false;
		for(int j = 0; j < n - 1; j++) {
			if(a[j] > a[j + 1]) {
				check = true;
				swap(a[j + 1], a[j]);
			}
		}
		if(!check) break;
		cout << "Buoc " << i + 1 << ": ";
		for(int k = 0; k < n; k++) {
			cout << a[k] << " ";
		}
		cout << endl;
	}
}
