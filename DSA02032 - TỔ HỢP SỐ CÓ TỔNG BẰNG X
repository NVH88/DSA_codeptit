#include<bits/stdc++.h>
using namespace std;

vector<string> v;
int n, s, a[100];

void Try(int i, int sum, string x) {
	for(int j = i; j <= n; ++j) {
		if(sum + a[j] == s) {
			v.push_back("{" + x + to_string(a[j]) + "}");
		}
		if(sum + a[j] < s) {
			Try(j, sum + a[j], x + to_string(a[j]) + " ");
		}
	}
}

int main() {
	int t; cin >> t;
	while(t--) {
		v.clear();
		cin >> n >> s;
		for(int i = 1; i <= n; ++i) cin >> a[i];
		Try(1, 0, "");
		if(!v.size()) {
			cout << -1 << endl;
			continue;
		}
		cout << v.size() << " ";
		for(string i : v) cout << i << " ";
		cout << endl;
	}
}
