#include<bits/stdc++.h>
using namespace std;

int main() {
	int c, n; cin >> c >> n;
	int a[n], dp[c + 1], ans = 0;
	memset(dp, 0, sizeof(dp));
	for(int &i : a) cin >> i;
	dp[0] = 1;
	for(int i = 0; i < n; i++) {
		for(int j = c; j >= a[i]; j--) {
			if(dp[j - a[i]]) {
				dp[j] = 1;
				ans = max(ans, j);
			}
		}
	}
	cout << ans;
}
