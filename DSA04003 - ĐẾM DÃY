#include <bits/stdc++.h>
using namespace std;
#define ll long long
const int mod = 1e9 + 7;

ll rev(ll a) {
    ll x = 0;
    while(a) {
        x *= 10;
        x += a % 10;
        a /= 10;
    }
    return x;
}

ll pow1(ll a, ll b) {
    ll res = 1;
    while(b) {
        if(b % 2 == 1) {
            res *= a;
            res %= 123456789;
        }
        a *= a;
        a %= 123456789;
        b /= 2;
    }
    return res;
}

int main() {
	int t; cin >> t;
	while(t--) {
	    ll n;
	    cin >> n;
	    cout << pow1(2, n - 1) << endl;	
	}
}
