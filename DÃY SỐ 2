#include<bits/stdc++.h>
using namespace std;

int main() {
	int t; cin >> t;
	while(t--) {
		int n; cin >> n;
		vector<int> v;
		vector<string> vt;
		for(int i = 0; i < n; ++i) {
			int x; cin >> x;
			v.push_back(x);
		}
		while(v.size() > 1) {
			string s = "[";
			for(int i = 0; i < v.size() - 1; ++i) s += to_string(v[i]) + " ";
			s += to_string(v[v.size() - 1]) + "] ";
			vt.push_back(s);
			vector<int> tmp;
			for(int i = 1; i < v.size(); ++i) {
				tmp.push_back(v[i] + v[i - 1]);
			}
			v = tmp;
		}
		cout << "[" << v[0] << "] ";
		for(int i = vt.size() - 1; i >= 0; --i) cout << vt[i];
		cout << endl;
	}
}
