#include<bits/stdc++.h>
using namespace std;

int main() {
	int t; cin >> t;
	while(t--) {
		int n; cin >> n;
		vector<int> v;
		for(int i = 0; i < n; i++) {
			int x; cin >> x;
			v.push_back(x);
		}
		while(v.size() > 1) {
			cout << "[";
			for(int i = 0; i < v.size() - 1; i++) cout << v[i] << " ";
			cout << v[v.size() - 1] << "]" << endl;
			vector<int> tmp;
			for(int i = 1; i < v.size(); i++) {
				tmp.push_back(v[i] + v[i - 1]);
			}
			v = tmp;
		}
		cout << "[" << v[0] << "]" << endl;
	}
}
