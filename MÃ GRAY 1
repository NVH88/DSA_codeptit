#include<bits/stdc++.h>
using namespace std;

int main() {
	int t; cin >> t;
	while(t--) {
		int n; cin >> n;
		vector<string> v;
		v.push_back("0"); v.push_back("1");
		while(--n) {
			for(int i = v.size() - 1; i >= 0; --i) {
				v.push_back("1" + v[i]);
				v[i] = "0" + v[i];
			}
		}
		for(string s : v) cout << s << " ";
		cout << endl;
	}
}
