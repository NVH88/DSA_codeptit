#include<bits/stdc++.h>
using namespace std;

int main() {
	int t;
	cin >> t;
	while(t--) {
		int m, n;
		cin >> m >> n;
		set<int> s1;
		set<int> s2;
		for(int i = 0; i < m; i++) {
			int a;
			cin >> a;
			s1.insert(a);
		}
		for(int i = 0; i < n; i++) {
			int a;
			cin >> a;
			if(s1.find(a) != s1.end()) s2.insert(a);
			s1.insert(a);
		}
		for(int i : s1) cout << i << " ";
		cout << endl;
		for(int i : s2) cout << i << " ";
		cout << endl;
	}
}
