#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin >> t;
    while(t--) {
		string s; cin >> s;
		int cnt[300] = {0};
		for(int i = 0; i < s.length(); i++) {
			cnt[s[i]]++;
		}
		if((s.length() + 1) / 2 >= *max_element(cnt, cnt + 300)) cout << 1 << endl;
		else cout << -1 << endl;
	}
}
