#include<bits/stdc++.h>
using namespace std;

int d[105][105], n, m;

void floyd() {
	for(int k = 1; k <= n; ++k) {
		for(int i = 1; i <= n; ++i) {
			for(int j = 1; j <= n; ++j) {
				d[i][j] = min(d[i][j], d[i][k] + d[k][j]);
			}
		}
	}
}

int main() {
	int t; cin >> t;
	while(t--) {
		cin >> n >> m;
		for(int i = 1; i <= n; ++i) {
			for(int j = 1; j <= n; ++j) {
				if(i != j) d[i][j] = 1e9;
				else d[i][j] = 0;
			}
		}
		for(int i = 0; i < m; ++i) {
			int x, y; cin >> x >> y;
			d[x][y] = 1;
		}
		floyd();
		double ans = 0; int cnt = 0;
		for(int i = 1; i <= n; ++i) {
			for(int j = 1; j <= n; ++j) {
				if(d[i][j] != 1e9 && d[i][j] != 0) ans += d[i][j], ++cnt;
			}
		}
		cout << fixed << setprecision(2) << (double)(ans / cnt) << endl;
	}
}
