#include<bits/stdc++.h>
using namespace std;
#define ll long long

ll calc(ll a, ll b, char c) {
	if(c == '+') return a + b;
	if(c == '-') return a - b;
	if(c == '*') return a * b;
	return a / b;
}

int ut(char c) {
	if(c == '+' || c == '-') return 1;
	if(c == '*' || c == '/') return 2;
	return 0;
}

int main() {
	int t; cin >> t;
	while(t--) {
		string s; cin >> s;
		stack<ll> a;
		stack<char> b;
		for(int i = 0; i < s.length(); ++i) {
			if(isdigit(s[i])) {
				int tmp = 0;
				while(isdigit(s[i])) {
					tmp *= 10;
					tmp += s[i] - '0';
					++i;
				}
				--i;
				a.push(tmp);
			}
			else if(s[i] == '(') b.push(s[i]);
			else if(s[i] == ')') {
				while(b.top() != '(') {
					ll x = a.top(); a.pop();
					ll y = a.top(); a.pop();
					char z = b.top(); b.pop();
					a.push(calc(y, x, z));
				}
				b.pop();
			}
			else {
				while(b.size() && ut(b.top()) >= ut(s[i])) {
					ll x = a.top(); a.pop();
					ll y = a.top(); a.pop();
					char z = b.top(); b.pop();
					a.push(calc(y, x, z));
				}
				b.push(s[i]);
			}
		}
		while(b.size()) {
			ll x = a.top(); a.pop();
			ll y = a.top(); a.pop();
			char z = b.top(); b.pop();
			a.push(calc(y, x, z));
		}
		cout << a.top() << endl;
	}
}
