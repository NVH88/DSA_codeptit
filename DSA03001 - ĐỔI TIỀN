#include<bits/stdc++.h>
using namespace std;

int tien[10] = {1000, 500, 200, 100, 50, 20, 10, 5, 2, 1};

int main() {
	int t;
	cin >> t;
	while(t--) {
		int n;
		cin >> n;
		int cnt = 0;
		for(int i = 0; i < 10; i++) {
			if(n >= tien[i]) {
				cnt += n / tien[i];
				n %= tien[i];	
			}
		}
		cout << cnt << endl;
	}
}
