#include<bits/stdc++.h>
using namespace std;

int main() {
	int t; cin >> t;
	while(t--) {
		int n; cin >> n;
		vector<int> v1, v2;
		for(int i = 0; i < n; i++) {
			int x; cin >> x;
			v1.push_back(x);
			v2.push_back(i + 1);
		}
		int cnt = 1;
		while(1) {
			if(v1 == v2) {
				cout << cnt << endl;
				break;
			}
			else {
				++cnt;
				next_permutation(v2.begin(), v2.end());
			}
		}
	}
}
