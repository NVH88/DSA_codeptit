#include<bits/stdc++.h>
using namespace std;

int n, a[20], x[20];
vector<vector<int>> v;
int p[1505];

void sang() {
	memset(p, 1, sizeof(p));
	p[0] = p[1] = 0;
	for(int i = 2; i < sqrt(1505); i++) {
		if(p[i]) {
			for(int j = i * i; j <= 1505; j += i) {
				p[j] = 0;
			}
		}
	}
}

bool cmp(int x, int y) {
	return x > y;
}

void ktao() {
	v.clear();
	cin >> n;
	for(int i = 1; i <= n; i++) cin >> a[i];
	sort(a + 1, a + 1 + n, cmp);
}

void Try(int i, int sum, int pos) {
	if(p[sum]) {
		vector<int> tmp(x + 1, x + i);
		v.push_back(tmp);
	}
	for(int j = pos; j <= n; j++) {
		x[i] = a[j];
		Try(i + 1, sum + a[j], j + 1);
	}
}

int main() {
	sang();
	int t; cin >> t;
	while(t--) {
		ktao();
		Try(1, 0, 1);
		sort(v.begin(), v.end());
		for(auto it : v) {
			for(int i : it) {
				cout << i << " ";
			}
			cout << endl;
		}
	}
}
