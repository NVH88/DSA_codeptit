#include<bits/stdc++.h>
using namespace std;

int n, k, a[20], b[20], cnt, ok;

bool check() {
	for(int i = 1; i <= k; i++) {
		if(a[i] != b[i]) return false;
	}
	return true;
}

void Try(int i) {
	for(int j = a[i - 1] + 1; j <= n; j++) {
		if(ok) return;
		a[i] = j;
		if(i == k) {
			++cnt;
			if(check()) {
				ok = 1;
				cout << cnt << endl;
			}
		}
		else Try(i + 1);
	}
}

int main() {
	int t; cin >> t;
	while(t--) {
		cnt = 0;
		ok = 0;
		cin >> n >> k;
		for(int i = 1; i <= k; i++) cin >> b[i];
		Try(1);
	}
}
