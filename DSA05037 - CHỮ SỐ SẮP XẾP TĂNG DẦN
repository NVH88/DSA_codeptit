#include<bits/stdc++.h>
using namespace std;
const int M = 1e9 + 7;

int main() {
	int t; cin >> t;
	while(t--) {
		int n; cin >> n;
		long long dp[n + 1][10] = {0};
		for(int i = 0; i < 10; ++i) dp[1][i] = 1;
		for(int i = 2; i <= n; ++i) {
			for(int j = 0; j < 10; ++j) {
				for(int k = j; k < 10; ++k) {
					dp[i][j] += dp[i - 1][k];
					dp[i][j] %= M;
				}
			}
		}
		long long ans = 0;
		for(int i = 0; i < 10; ++i) ans += dp[n][i], ans %= M;
		cout << ans << endl;
	}
}
