#include <bits/stdc++.h>
using namespace std;
#define ll long long
const int mod = 1e9 + 7;

int main() {
	int t; cin >> t;
	while(t--) {
	    int n, m, x;
	    cin >> n >> m >> x;
	    int a[n + m + 1];
	    for(int i = 1; i <= n; i++) cin >> a[i];
	    for(int i = n + 1; i <= n + m; i++) cin >> a[i];
	    sort(a + 1, a + n + m + 1);
	    cout << a[x] << endl;
	}
}
