#include<bits/stdc++.h>
using namespace std;

struct node {
	int data;
	node *l, *r;
	node(int x) {
		data = x;
		l = r = NULL;
	}
};

void insert(node *root, int u, int v, char c) {
	if(root == NULL) return;
	if(root->data == u) {
		if(c == 'L') root->l = new node(v);
		else root->r = new node(v);
		return;
	}
	insert(root->l, u, v, c);
	insert(root->r, u, v, c);
}

void level(node *root) {
	queue<node *> q;
	q.push(root);
	cout << root->data << " ";
	while(q.size()) {
		node *t = q.front(); q.pop();
		if(t->l != NULL) cout << t->l->data << " ", q.push(t->l);
		if(t->r != NULL) cout << t->r->data << " ", q.push(t->r);
	}
}

int main() {
	int t; cin >> t;
	while(t--) {
		int n; cin >> n;
		node *root = NULL;
		for(int i = 0; i < n; ++i) {
			int a, b; char c; cin >> a >> b >> c;
			if(root == NULL) root = new node(a);
			insert(root, a, b, c);
		}
		level(root);
		cout << endl;
	}
}
