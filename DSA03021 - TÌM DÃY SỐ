#include<bits/stdc++.h>
using namespace std;

bool check(int a, int b) {
	int c = a / (b + 1) + 1;
	return a / c == b;
}

int main() {
	int n; cin >> n;
	int a[n], m = 1e9;
	for(int i = 0; i < n; i++) {
		cin >> a[i];
		m = min(m, a[i]);
	} 
	while(m--) {
		int ans = 0;
		bool ck = true;
		for(int i = 0; i < n; i++) {
			ans += a[i] / (m + 2) + 1;
			if(!check(a[i], m + 1)) {
				ck = false;
				break;
			}
		}
		if(ck) {
			cout << ans;
			break;
		}
	}
}
