#include<bits/stdc++.h>
using namespace std;

bool cmp(int a, int b) {
	return a > b;
}

int main() {
	int t;
	cin >> t;
	while(t--) {
		int d;
		cin >> d;
		string s;
		cin >> s;
		int n = s.length();
		map<char, int> mp;
		for(int i = 0; i < n; i++) {
			mp[s[i]]++;
		}
		vector<int> v;
		for(auto i : mp) {
			v.push_back(i.second);
		}
		sort(v.begin(), v.end(), cmp);
		bool used[n], check = false;
		memset(used, false, sizeof(used));
		for(int it : v) {
			int idx;
			for(int i = 0; i < n; i++) {
				if(!used[i]) {
					idx = i;
					break;
				}
			}
			for(int i = idx; idx < n; idx += d) {
				if(it > 0) {
					if(used[idx]) {
						check = true;
					} 
					else {
						used[idx] = true;
						--it;
					}
				}
				else break;
			}
			if(it > 0) check = true;
		}
		if(check) cout << -1 << endl;   
		else cout << 1 << endl;
	}
}
