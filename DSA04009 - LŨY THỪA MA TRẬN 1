#include<bits/stdc++.h>
using namespace std;
#define ll long long
const int mod = 1e9 + 7;
int n;

struct matrix {
	ll a[15][15];
};

matrix nhan(matrix x, matrix y) {
	matrix res;
	for(int i = 0; i < n; i++) {
		for(int j = 0; j < n; j++) {
			res.a[i][j] = 0;
			for(int k = 0; k < n; k++) {
				res.a[i][j] += x.a[i][k] * y.a[k][j];
				res.a[i][j] %= mod;
			}
		}
	}
	return res;
}

matrix lt(matrix x, int k) {
	if(k == 1) return x;
	matrix tmp = lt(x, k / 2);
	tmp = nhan(tmp, tmp);
	if(k % 2 == 1) return nhan(tmp, x);
	return tmp;
}

int main() {
	int t; cin >> t;
	while(t--) {
		int k;
		cin >> n >> k;
		matrix x;
		for(int i = 0; i < n; i++) {
			for(int j = 0; j < n; j++) {
				cin >> x.a[i][j];
			}
		}
		x = lt(x, k);
		for(int i = 0; i < n; i++) {
			for(int j = 0; j < n; j++) {
				cout << x.a[i][j] << " ";
			}
			cout << endl;
		}
	}
}
