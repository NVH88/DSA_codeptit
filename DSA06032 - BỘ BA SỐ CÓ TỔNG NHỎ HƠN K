#include<bits/stdc++.h>
using namespace std;
#define ll long long

int main() {
	int t;
	cin >> t;
	while(t--) {
		int n, k;
		cin >> n >> k;
		ll a[n + 1];
		for(int i = 1; i <= n; i++) cin >> a[i];
		sort(a + 1, a + 1 + n);
		ll cnt = 0;
		for(int i = 1; i <= n - 2; i++) {
			for(int j = i + 1; j <= n - 1; j++) {
				auto it = lower_bound(a + j + 1, a + n + 1, k - a[i] - a[j]);
				cnt += (it - a) - 1 - j;
			}
		}
		cout << cnt << endl;
	}
}
