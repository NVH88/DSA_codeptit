#include<bits/stdc++.h>
using namespace std;

int n, a[15], used[15];

bool check(int x, int y, int z) {
	if(x == -1) return true;
	if((y == 0 || y == 4) && (x != 0 && x != 4 && z != 0 && z != 4)) return false;
	return true;
}

void Try(int i, int t1, int t2) {
	for(int j = 0; j <= n; ++j) {
		if(!used[j] && check(t1, t2, j)) {
			used[j] = 1;
			a[i] = j;
			if(i == n) {
				for(int k = 0; k <= n; ++k) {
					cout << (char)('A' + a[k]);
				}
				cout << endl;
			}
			else Try(i + 1, t2, j);
			used[j] = 0;
		}
	}
}

int main() {
	char x; cin >> x;
	n = x - 'A';
	Try(0, -1, -1);
}
