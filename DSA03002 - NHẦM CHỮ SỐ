#include<bits/stdc++.h>
using namespace std;

string ma(string s) {
	for(int i = 0; i < s.length(); i++) {
		if(s[i] == '5') s[i] = '6';
	}
	return s;
}

string mi(string s) {
	for(int i = 0; i < s.length(); i++) {
		if(s[i] == '6') s[i] = '5';
	}
	return s;
}

int main() {
	string s1, s2;
	cin >> s1 >> s2;
	cout << stoi(mi(s1)) + stoi(mi(s2)) << " " << stoi(ma(s1)) + stoi(ma(s2));
}
