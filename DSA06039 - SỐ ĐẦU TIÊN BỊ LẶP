#include <bits/stdc++.h>
using namespace std;
#define ll long long

int main() {
    int t; cin >> t;
    while(t--) {
    	int n; cin >> n;
    	int a[n];
    	map<int, int> mp;
    	bool check = false;
    	for(int i = 0; i < n; i++) {
    		cin >> a[i];
    		mp[a[i]]++;
		}
		for(int i = 0; i < n; i++) {
			if(mp[a[i]] > 1) {
				check = true;
				cout << a[i] << endl;
				break;
			}
		}
		if(!check) cout << "NO" << endl;
	}
}
