#include<bits/stdc++.h>
using namespace std;

int n, k, a[20], x[20], dem;

void Try(int i, int cnt) {
	for(int j = i + 1; j <= n; j++) {
		if(a[j] > a[i]) {
			if(cnt + 1 == k) ++dem;
			else Try(j, cnt + 1);
		}
	}
}

int main() {
	cin >> n >> k;
	for(int i = 1; i <= n; i++) cin >> a[i];
	for(int i = 1; i <= n - k; i++) Try(i, 1);
	cout << dem;
}
