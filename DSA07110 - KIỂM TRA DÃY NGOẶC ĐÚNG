#include<bits/stdc++.h>
using namespace std;

bool check(string s) {
	stack<char> st;
	for(char i : s) {
		if(i == '(' || i == '[' || i == '{') st.push(i);
		else {
			if(i == ')') {
				if(!st.size() || st.top() != '(') return false;
				st.pop(); 
			}
			else if(i == ']') {
				if(!st.size() || st.top() != '[') return false;
				st.pop(); 
			}
			else {
				if(!st.size() || st.top() != '{') return false;
				st.pop(); 
			} 
		}
	}
	return st.empty();
}

int main() {
	int t; cin >> t;
	while(t--) {
		string s; cin >> s;
		if(check(s)) cout << "YES" << endl;
		else cout << "NO" << endl;
	}
}
