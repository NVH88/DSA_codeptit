#include<bits/stdc++.h>
using namespace std;

vector<int> ke[1005];
int n, m, ok, color[1005];

bool bfs(int u) {
	queue<int> q;
	q.push(u);
	color[u] = 1;
	while(q.size()) {
		int f = q.front(); q.pop();
		for(int i : ke[f]) {
			if(!color[i]) {
				if(color[f] == 1) color[i] = 2;
				else color[i] = 1;
				q.push(i);
			}
			else if(color[i] == color[f]) return true;
		}
	}
	return false;
}

int main() {
	int t; cin >> t;
	while(t--) {
		ok = 0;
		cin >> n >> m;
		for(int i = 1; i <= n; ++i) ke[i].clear();
		memset(color, 0, sizeof(color));
		for(int i = 0; i < m; ++i) {
			int a, b; cin >> a >> b;
			ke[a].push_back(b); ke[b].push_back(a);
		}
		for(int i = 1; i <= n; ++i) {
			if(!color[i]) {
				if(bfs(i)) {
					ok = 1;
					cout << "NO" << endl;	
					break;	
				}
			}
		}
		if(!ok) cout << "YES" << endl;
	}
}
