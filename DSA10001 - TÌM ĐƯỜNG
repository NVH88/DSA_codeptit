#include<bits/stdc++.h>
using namespace std;

int n, m, a, b, ok;
int dx[] = {+0, -1, +1, +0};
int dy[] = {-1, +0, +0, +1};
char s[505][505];

void dfs(int u, int v, int h, int cnt) {
	if(cnt > 2 || ok) return;
	if(s[u][v] == 'T') {
		ok = 1; return;
	}
	s[u][v] = '*';
	for(int k = 0; k < 4; ++k) {
		int i = u + dy[k], j = v + dx[k];
		if(i >= 0 && i < n && j >= 0 && j < m && s[i][j] != '*') {
			if(h == -1) {
				if(k == 0 || k == 3) dfs(i, j, 1, cnt);
				else dfs(i, j, 0, cnt);
			}
			else if(h == 0) {
				if(k == 1 || k == 2) dfs(i, j, 0, cnt);
				else dfs(i, j, 1, cnt + 1);
			}
			else {
				if(k == 1 || k == 2) dfs(i, j, 0, cnt + 1);
				else dfs(i, j, 1, cnt);
			}
		}
	}
	s[u][v] = '.';
}

int main() {
	int t; cin >> t;
	while(t--) {
		ok = 0;
		cin >> n >> m;
		for(int i = 0; i < n; ++i) {
			for(int j = 0; j < m; ++j) {
				cin >> s[i][j];
				if(s[i][j] == 'S') a = i, b = j;
			}
		}
		dfs(a, b, -1, 0);
		if(ok) cout << "YES" << endl;
		else cout << "NO" << endl;
	}
}
