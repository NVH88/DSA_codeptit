#include <bits/stdc++.h>
using namespace std;

const int MAXN = 105;

multimap <double, int> mp;

void solve(vector<int> &c, vector<int> &w, int W, int n){
    double maxx = 0;
    for(int i = 0; i < n; i++){
        mp.insert({(double) c[i] / w[i], i});
    }
    for(multimap <double, int> :: reverse_iterator it = mp.rbegin(); it != mp.rend(); ++it){
        double fraction = (double) W / w[it->second];
        if(W >= 0 && W >= w[it->second]){
            maxx += c[it->second];
            W -= w[it->second];
        }else if(W < w[it->second]){
            maxx += fraction * c[it->second];
            break;
        }
    }
    cout << fixed << setprecision(2) << maxx << endl;
}

int main(){
    int T; cin >> T;
    while(T--){
        mp.clear();
        int n, W; cin >> n >> W;
        vector<int> c(n), w(n);
        for(int i = 0; i < n; i++) cin >> c[i] >> w[i];
        solve(c, w, W, n);
    }
    return 0;
}
