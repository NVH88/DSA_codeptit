#include<bits/stdc++.h>
using namespace std;
#define ll long long

vector<int> ke[1005];
bool visited[1005];
int parent[1005];

void BFS(int u) {
	queue<int> q;
	q.push(u);
	visited[u] = true;
	while(!q.empty()) {
		int t = q.front(); q.pop();
		for(int i : ke[t]) {
			if(!visited[i]) {
				visited[i] = true;
				parent[i] = t;
				q.push(i);
			}
		}
	}
}

int main() {
	int t;
	cin >> t;
	while(t--) {
		int n, m, u, v;
		cin >> n >> m >> u >> v;
		memset(visited, false, sizeof(visited));
		memset(parent, 0, sizeof(parent));
		for(int i = 0; i < 1005; i++) ke[i].clear();
		for(int i = 1; i <= m; i++) {
			int x, y; cin >> x >> y;
			ke[x].push_back(y);
			ke[y].push_back(x);
		}
		for(int i = 1; i <= n; i++) sort(ke[i].begin(), ke[i].end());
		BFS(u);
		vector<int> c;
		if(!visited[v]) cout << -1 << endl;
		else {
			while(v != u) {
				c.push_back(v);
				v = parent[v];
			}
			c.push_back(u);
			reverse(c.begin(), c.end());
			for(int i : c) cout << i << " ";
			cout << endl;
		}
	}
}
