#include<bits/stdc++.h>
using namespace std;

int main() {
	int t; cin >> t;
	while(t--) {
		int n; cin >> n;
		int a[n], b[n], c[n];
		for(int &i : a) cin >> i;
		stack<int> st;
		for(int i = n - 1; i >= 0; --i) {
			while(st.size() && a[i] >= a[st.top()]) st.pop();
			if(st.size()) b[i] = st.top();
			else b[i] = -1;
			st.push(i);
		}
		while(st.size()) st.pop();
		for(int i = n - 1; i >= 0; --i) {
			while(st.size() && a[i] <= a[st.top()]) st.pop();
			if(st.size()) c[i] = st.top();
			else c[i] = -1;
			st.push(i);
		}
		for(int i = 0; i < n; ++i) {
			if(b[i] == -1 || c[b[i]] == -1) cout << -1 << " ";
			else cout << a[c[b[i]]] << " ";
		}
		cout << endl;
	}
}
