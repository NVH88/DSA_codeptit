#include<bits/stdc++.h>
using namespace std;

int main() {
	int t; cin >> t;
	while(t--) {
		int n; cin >> n;
		int dp[n], i = 0, j = 0, k = 0; dp[0] = 1;
		for(int x = 1; x < n; ++x) {
			dp[x] = min({dp[i] * 2, dp[j] * 3, dp[k] * 5});
			if(dp[x] == dp[i] * 2) ++i;
			if(dp[x] == dp[j] * 3) ++j;
			if(dp[x] == dp[k] * 5) ++k;
		}
		cout << dp[n - 1] << endl;
	}
}
