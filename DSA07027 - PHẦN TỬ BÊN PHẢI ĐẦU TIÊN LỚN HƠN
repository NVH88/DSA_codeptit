#include<bits/stdc++.h>
using namespace std;

int main() {
	int t; cin >> t;
	while(t--) {
		int n; cin >> n;
		int a[n], b[n]; 
		for(int &i : a) cin >> i;
		stack<int> st;
		for(int i = n - 1; i >= 0; --i) {
			while(st.size() && a[i] >= st.top()) st.pop();
			if(st.size()) b[i] = st.top();
			else b[i] = -1;
			st.push(a[i]);
		}
		for(int i = 0; i < n; ++i) cout << b[i] << " ";
		cout << endl;
	}
}
