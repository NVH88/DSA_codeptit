#include<bits/stdc++.h>
using namespace std;
set<pair<int, int>> canh;

int main() {
	int n;
	cin >> n;
	cin.ignore();
	for(int i = 1; i <= n; i++) {
		string s; 
		getline(cin, s);
		stringstream ss(s);
		string tmp;
		while(ss >> tmp) {
			int x = stoi(tmp);
			canh.insert({min(i, x), max(i, x)});
		}
	}
	for(auto i : canh) cout << i.first << " " << i.second << endl;
}
