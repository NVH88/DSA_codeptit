#include<bits/stdc++.h>
using namespace std;

int main() {
	int t;
	cin >> t;
	while(t--) {
		int n, m, k;
		cin >> n >> m >> k;
		map<int, int> mp1;
		map<int, int> mp2;
		for(int i = 1; i <= n; i++) {
			int x;
			cin >> x;
			mp1[x]++;
		}
		for(int i = 1; i <= m; i++) {
			int x;
			cin >> x;
			mp2[x]++;
		}
		bool check = false;
		vector<int> v;
		for(int i = 1; i <= k; i++) {
			int x;
			cin >> x;
			if(mp1[x] > 0 && mp2[x] > 0) {
				check = true;
				v.push_back(x);
				--mp1[x]; --mp2[x];
			}
		}
		if(!check) cout << "NO";
		else for(int i : v) cout << i << " ";
		cout << endl;
	}
}
