#include<bits/stdc++.h>
using namespace std;

int n, k, a[10], used[10], res;
string s[10];

string xl(string s) {
	string tmp = s;
	for(int i = 0; i < k; ++i) {
		tmp[i] = s[a[i]];
	}
	return tmp;
}

void Try(int i) {
	for(int j = 0; j < k; ++j) {
		if(!used[j]) {
			used[j] = 1;
			a[i] = j;
			if(i == k - 1) {
				string tmp[10];
				for(int x = 1; x <= n; ++x) {
					tmp[x] = xl(s[x]);
				}
				sort(tmp + 1, tmp + n + 1);
				res = min(res, stoi(tmp[n]) - stoi(tmp[1]));
			}
			else Try(i + 1);
			used[j] = 0;
		}
	}
}

int main() {
	res = 1e9;
	cin >> n >> k;
	for(int i = 1; i <= n; ++i) cin >> s[i];
	Try(0);
	cout << res;
}
