#include<bits/stdc++.h>
using namespace std;

int main() {
	int t; cin >> t;
	while(t--) {
		string s; cin >> s;
		stack<int> st;
		st.push(1);
		int cnt = 2;
		for(char i : s) {
			if(i == 'D') st.push(cnt++);
			else {
				while(st.size()) cout << st.top(), st.pop();
				st.push(cnt++);
			}
		}
		while(st.size()) cout << st.top(), st.pop();
		cout << endl;
	}
}
