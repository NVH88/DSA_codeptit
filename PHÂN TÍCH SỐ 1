#include<bits/stdc++.h>
using namespace std;

int main() {
	int t; cin >> t;
	while(t--) {
		int n; cin >> n;
		vector<int> v;
		v.push_back(n);
		while(v[0] != 1) {
			cout << "(";
			for(int i = 0; i < v.size() - 1; i++) cout << v[i] << " ";
			cout << v[v.size() - 1] << ") ";
			int i = v.size() - 1, cnt = 0;
			while(v[i] == 1) {
				--i; ++cnt;
				v.pop_back();
			}
			--v[i]; ++cnt;
			int x = cnt / v[i], y = cnt % v[i];
			while(x--) {
				v.push_back(v[i]);
			}
			if(y != 0) v.push_back(y);
		}
		--n;
		cout << "(";
		while(n--) cout << "1 ";
		cout << 1 << ")";
		cout << endl;
	}
}
