#include<bits/stdc++.h>
using namespace std;

int n, k;
string s;
set<string> se;

//bool check(string s) {
//	stack<char> st;
//	for(char i : s) {
//		if(i == '(') st.push(i);
//		else if(i == ')'){
//			if(st.size()) return true;
