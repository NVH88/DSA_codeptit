#include <bits/stdc++.h>
using namespace std;
#define ll long long
const int M = 1e9 + 7;
ll dp[105][50005];
int n, s; 

void ktao() {
	for(int i = 0; i <= 9; i++) dp[1][i] = 1;
	for(int i = 2; i <= 101; i++) {
		for(int j = 1; j <= i * 9; j++) {
			for(int k = 0; k <= 9; k++) {
				if(j > k) {
					dp[i][j] += dp[i - 1][j - k]; 
					dp[i][j] %= M;
				}
			}
		}
	}
}

int main() {
	ktao();
	int t; cin >> t;
	while(t--) {
		cin >> n >> s;
		cout << dp[n][s] << endl;
	}
}
