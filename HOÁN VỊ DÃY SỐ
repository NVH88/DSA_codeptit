#include<bits/stdc++.h>
using namespace std;

int n, a[20], x[20], used[20];

void Try(int i) {
	for(int j = 1; j <= n; j++) {
		if(!used[j]) {
			used[j] = 1;
			a[i] = j;
			if(i == n) {
				for(int k = 1; k <= n; k++) {
					cout << x[a[k]] << " ";
				}
				cout << endl;
			}
			else Try(i + 1);
			used[j] = 0;
		}
	}
}

int main() {
	cin >> n;
	for(int i = 1; i <= n; i++) cin >> x[i];
	sort(x + 1, x + n + 1);
	Try(1);
}
