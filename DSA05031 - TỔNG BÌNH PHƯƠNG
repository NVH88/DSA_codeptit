#include<bits/stdc++.h>
using namespace std;

int main() {
	int t; cin >> t;
	while(t--) {
		int n; cin >> n;
		int dp[n + 1];
		for(int i = 1; i <= n; ++i) dp[i] = 1e9;
		dp[0] = 0;
		for(int i = 0; i <= 100; ++i) {
			for(int j = 1; j <= n; ++j) {
				if(j >= i * i) dp[j] = min(dp[j], dp[j - i * i] + 1);
			}
		}
		cout << dp[n] << endl;
	}
}
