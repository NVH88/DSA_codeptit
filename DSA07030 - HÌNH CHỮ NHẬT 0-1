#include<bits/stdc++.h>
using namespace std;

int main() {
	int t; cin >> t;
	while(t--) {
		int n, m; cin >> n >> m;
		int a[n][m];
		for(int i = 0; i < n; ++i) {
			for(int j = 0; j < m; ++j) {
				cin >> a[i][j];
				if(i > 0 && a[i - 1][j] && a[i][j]) a[i][j] = a[i - 1][j] + 1; 
			}
		}
		int ans = 0;
		for(int i = 0; i < n; ++i) {
			stack<int> st;
			int b[m], c[m];
			for(int j = 0; j < m; ++j) {
				while(st.size() && a[i][st.top()] >= a[i][j]) st.pop();
				if(st.size()) b[j] = st.top();
				else b[j] = -1;
				st.push(j);
			}
			while(st.size()) st.pop();
			for(int j = m - 1; j >= 0; --j) {
				while(st.size() && a[i][st.top()] >= a[i][j]) st.pop();
				if(st.size()) c[j] = st.top();
				else c[j] = m;
				st.push(j);
			}
			for(int j = 0; j < m; ++j) {
				ans = max(ans, a[i][j] * (c[j] - b[j] - 1));
			}
		}
		cout << ans << endl;
	}
}
