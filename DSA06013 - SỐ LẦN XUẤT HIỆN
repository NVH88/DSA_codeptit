#include<bits/stdc++.h>
using namespace std;

bool cmp(int a, int b) {
	return a > b;
}

int main() {
	int t;
	cin >> t;
	while(t--) {
		int n, k;
		cin >> n >> k;
		map<int, int> mp;
		for(int i = 0; i < n; i++) {
			int a;
			cin >> a;
			mp[a]++;
		}
		if(mp[k]) cout << mp[k] << endl;
		else cout << -1 << endl;
	}
}
